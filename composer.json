  "type": "library",
  "require": {
    "grpc/grpc": "^v1.1.0",
    "guzzlehttp/guzzle": "^6.3",
    "mdanter/ecc": "^0.5.0",
    "google/protobuf": "^3.6.0",
    "vendasta/vax": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Vendasta\\Business\\V1\\": "./src",
      "Business\\V1\\": "./generated/grpc/Business/V1",
      "GPBMetadata\\Business\\V1\\": "./generated/grpc/GPBMetadata/Business/V1"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
